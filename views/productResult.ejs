<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Product List</title>
</head>
<body>
    <h2>All Product</h2>
    <table border="1">
        <thead>
            <th>Product #</th>
            <th>Product Name</th>
            <th>Product Price</th>
        </thead>
        <% productList.forEach (function(data){ %>
            <tr>
                <td> <%= data.pid %> </td>
                <td> <%= data.name %> </td>
                <td> <%= data.price %> </td>
            </tr>
       <% }) %>
    </table>
</body>
</html>